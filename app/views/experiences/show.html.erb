<%- model_class = Experience -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:m_income) %>:</strong></dt>
  <dd><%= @experience.m_income %></dd>
  <dt><strong><%= model_class.human_attribute_name(:f_income) %>:</strong></dt>
  <dd><%= @experience.f_income %></dd>
  <dt><strong><%= model_class.human_attribute_name(:m_start) %>:</strong></dt>
  <dd><%= @experience.m_start %></dd>
  <dt><strong><%= model_class.human_attribute_name(:m_total) %>:</strong></dt>
  <dd><%= @experience.m_total %></dd>
  <dt><strong><%= model_class.human_attribute_name(:f_start) %>:</strong></dt>
  <dd><%= @experience.f_start %></dd>
  <dt><strong><%= model_class.human_attribute_name(:f_total) %>:</strong></dt>
  <dd><%= @experience.f_total %></dd>
  <dt><strong><%= model_class.human_attribute_name(:helper) %>:</strong></dt>
  <dd><%= @experience.helper %></dd>
  <dt><strong><%= model_class.human_attribute_name(:location) %>:</strong></dt>
  <dd><%= @experience.location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd><%= @experience.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:body) %>:</strong></dt>
  <dd><%= @experience.body %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              experiences_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_experience_path(@experience), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              experience_path(@experience),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
